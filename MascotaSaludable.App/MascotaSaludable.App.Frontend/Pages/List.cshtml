            <td>Id Mascota</td>
            <td>Nombre Mascota</td>
            <td>Fecha de Nacimiento</td>
    </tr>
    @foreach (var Mascota in Model.Mascotas)
    {
        

        <tr>
            <td> </td>
            <td> @Mascota.IdentificacionDueno </td>
